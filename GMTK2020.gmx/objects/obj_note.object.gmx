          <argument>
            <kind>1</kind>
            <string>if (!stop) {
    x = obj_note_end.x + (distance_x * (frames_to_end / total_frames))
    y = view_yview + global.padding
    frames_to_end -= 1
} else {
    x = obj_note_end.x
    y = obj_note_end.y
}
    
// collision with obj_note_end
if (abs(x - obj_note_end.x) &lt; collision_buffer &amp;&amp; !stop) {
    // TEMP bonk sound effect
    //audio_play_sound(bonk, 20, false);
    
    alarm[0] = 60;
    stop = true;
    if (object_get_name(object_index) == "obj_note_up") {
        with (obj_player) {
            if (place_meeting(x, y + 1, obj_wall)) {
                instance_create(x, y + 32, obj_jump);
            }
        }
        obj_player.vsp = -1 * obj_player.jumpspeed
        sprite_index = spr_noteGone;
    } else if (object_get_name(object_index) == "obj_note_left") {
        obj_player.hsp = -obj_player.dashspeed
        obj_player.alarm[3] = 1;
        obj_player.alarm[11] = 6;
        obj_player.image_xscale = 1;
        sprite_index = spr_noteGone;
    } else if (object_get_name(object_index) == "obj_note_right") {
        obj_player.hsp = obj_player.dashspeed
        obj_player.alarm[3] = 1;
        obj_player.image_xscale = -1;
        obj_player.alarm[11] = 6;
        sprite_index = spr_noteGone;
    } else if (object_get_name(object_index) == "obj_note_shoot") {
        obj_player.alarm[0] = 1;
        sprite_index = spr_note_shootDone;
        
        // Play random vocal shout 
        //if (random(10) &gt;= 9) {
            sound_index = choose(comeon1, doit1, drop2, go2, ho1, hey1, oh1, yeah2)
            audio_play_sound(sound_index, 30, false)
        //}
    }
}



